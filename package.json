    },
    {
      "name": "Shubham Sharma"
    },
    {
      "name": "Sunderam Thakur"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ux-practice/iREST/issues"
  },
  "homepage": "https://github.com/ux-practice/iREST.git",
